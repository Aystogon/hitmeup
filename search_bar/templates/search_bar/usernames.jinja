{% extends "base.jinja" %}
{% block title %}
    Usernames
    {% endblock %}
{% block content %}
<form type="get" action=".">
    <input type="text" name="q">
    <button type="submit">Search</button>
</form>

{% for username in usernames %}
<h1>{{ username.username }}</h1>
<p>
    {{ username.username }}
</p>
{% endfor %}
{% endblock %}